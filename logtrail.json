{
    "index_patterns": [{
            "es": {
                "default_index": "log",
                "allow_url_parameter": false,
                "timezone": "CET"
            },
            "tail_interval_in_seconds": 10,
            "max_buckets": 500,
            "nested_objects": false,
            "tail_default_status": "Pause",
            "default_time_range_in_days": 0,
            "fields": {
                "mapping": {
                    "timestamp": "@timestamp",
                    "display_timestamp": "@timestamp",
                    "hostname": "hostname",
                    "program": "source",
                    "message": "message"
                }
            },
            "field_decorations": [{
                    "field": "hostname",
                    "pattern": "/(.*)/",
                    "replace": "<a ng-click=\"onHostSelected('$1')\">$1</a>"
                },
                {
                    "field": "program",
                    "pattern": "/(.*)/",
                    "replace": "<a ng-click=\"onProgramClick('$1')\">$1</a>:"
                },
                {
                    "field": "display_timestamp",
                    "pattern": "/(.*)/",
                    "replace": "<span><a target=\"es\" href=\"http://ss-log01:9200/log/carweb/_search?pretty=true&q=_id:{{event.id}}\">$1</a></span>"
                },
                {
                    "field": "message",
                    "pattern": "/([0-9a-fA-F]{8}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{12})/g",
                    "replace": "$1 (<a ng-click=\"onPatternClick('&quot;$1&quot;')\">L</a> <a ng-click=\"onPatternClick('&quot;$1&quot;', 'message')\">M</a>)"
                }
            ]
        },
        {
            "es": {
                "default_index": "message",
                "allow_url_parameter": false,
                "timezone": "CET"
            },
            "tail_interval_in_seconds": 10,
            "max_buckets": 500,
            "nested_objects": false,
            "tail_default_status": "Pause",
            "default_time_range_in_days": 0,
            "fields": {
                "mapping": {
                    "timestamp": "@timestamp",
                    "display_timestamp": "@timestamp",
                    "hostname": "sender",
                    "program": "contentType",
                    "message": "message"
                }
            },
            "field_decorations": [{
                    "field": "hostname",
                    "pattern": "/(.*)/",
                    "replace": "<a ng-click=\"onHostSelected('$1')\">$1</a>"
                },
                {
                    "field": "program",
                    "pattern": "/(.*)/",
                    "replace": "<a ng-click=\"onProgramClick('$1')\">$1</a>:"
                },
                {
                    "field": "display_timestamp",
                    "pattern": "/(.*)/",
                    "replace": "<span><a target=\"es\" href=\"http://ss-log01:9200/message/_search?pretty=true&q=_id:{{event.id}}\">$1</a></span>"
                },
                {
                    "field": "message",
                    "pattern": "/([0-9a-fA-F]{8}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{12})/g",
                    "replace": "$1 (<a ng-click=\"onPatternClick('&quot;$1&quot;')\">M</a> <a ng-click=\"onPatternClick('&quot;$1&quot;', 'log')\">L</a>)"
                }
            ]
        }
    ]
}